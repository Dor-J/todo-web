{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "NODE_ENV=development bun run start:local",
    "start:prod": "NODE_ENV=production bun run start:production",
    "------------------------------": "Using Angular configuration",
    "start:local": "ng serve",
    "start:development": "ng serve",
    "start:production": "ng serve -c production",
    "-------------------------------": "Using NODE_ENV",
    "build": "NODE_ENV=production ng build",
    "watch": "NODE_ENV=production ng build --watch --configuration development",
    "test": "NODE_ENV=test ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.5.1",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@ngx-env/builder": "^20.0.0",
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vitest": "^4.0.15"
  }
}